{
  "manifest_version": 2,

  "name": "Social Media Launcher",
  "description": "Launch your social media or other saved webpages through one chrome extension",
  "version": "4.2.0",
  "icons": { "128": "images/media-clipart-social-organization-4.png" },
  "browser_action": {
    "default_icon": "images/media-clipart-social-organization-4.png",
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab"]
}
